<%- include('../partials/header') -%>
<%- include('../partials/navbar') -%>
<h1>Current Students</h1>
<div class="row">
    <% if (students > 0) { %>
        <table class="striped">
            <thead>
                <tr>
                    <th>Student Name</th>
                    <th>Last Intervention Date</th>
                    <th>Current Intervention</th>
                    <th></th>
                </tr>
            </thead>
                <tbody>
                    <% for(var i=0; i<students.length; i++) { %>
                        <tr>
                            <td><%= students[i].firstName %> <% students[i].lastName %></td>
                            <td><%= interventionRecord.date %></td>
                            <td><%= interventionRecord.title %></td>
                            <td></td>
                        </tr>
                     <% } %>
                </tbody>
        </table>
        <%} else { %>
        <p>Please add a student to enter intervention data. </p>
        <% } %>
</div>
<%- include('../partials/footer') -%>